{
  "name": "dppresilhas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "minify-html": "html-minifier-terser src/index.html --collapse-whitespace --output=dist/index.html",
    "optimize-css": "postcss src/assets/css/main.css --use autoprefixer cssnano postcss-pxtorem --output dist/assets/css/main.css --no-map",
    "bundle": "browserify ./src/assets/js/app.js -o ./dist/assets/js/app.js",
    "transpile-js": "babel dist/assets/js/app.js -o dist/assets/js/app.js --presets=@babel/preset-env",
    "minify-js": "uglifyjs dist/assets/js/app.js --compress -o dist/assets/js/app.js",
    "build": "npm run minify-html && npm run optimize-css && npm run bundle && npm run transpile-js && npm run minify-js",
    "sass-start": "npx sass --watch ./src/assets/scss/main.scss ./src/assets/css/main.css",
    "sass-build": "npx sass ./src/assets/scss/main.scss ./src/assets/css/main.css"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "autoprefixer": "^10.4.18",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "cssnano": "^6.1.0",
    "html-minifier-terser": "^7.2.0",
    "imports-loader": "^5.0.0",
    "postcss": "^8.4.36",
    "postcss-cli": "^11.0.0",
    "postcss-pxtorem": "^6.1.0",
    "uglify-js": "^3.17.4"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  }
}
